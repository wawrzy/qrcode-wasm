{
	"compilerOptions": {
		"target": "es5",
		"strict": true,
		"declaration": true,
		"declarationDir": "./dist",
		"outDir": "./dist",
		"module": "esnext",
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
